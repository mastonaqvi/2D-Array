#include <iostream>
using namespace std;

int main() {
    int rows, col;
    int arr[5][7];
    
    cout << "Enter number of Rows: ";
    cin >> rows;
    cout << "Enter number of Columns: ";
    cin >> col;
    
    // Input values into the array
    cout << "Enter the elements of the array:" << endl;
    for(int i = 0; i < rows; i++) {
        for(int j = 0; j < col; j++) {
            cout << "Element [" << i << "][" << j << "]: ";
            cin >> arr[i][j];
        }
    }
    
    // Print the array elements
    cout << "The Array Elements are:" << endl;
    for(int i = 0; i < rows; i++) {
        for(int j = 0; j < col; j++) {
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
    
    return 0;
}
